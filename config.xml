<?xml version='1.0' encoding='utf-8'?>
<widget id="es.phonegap.push.sample" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>phonegap-plugin-push-sample</name>
    <description>
        A sample Apache Cordova application that responds to the deviceready event.
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Apache Cordova Team
    </author>
   <gap:plugin name="onesignal-cordova-plugin" spec="^2.4.1" source="npm" />	
<content src="index.html"/>
  <preference name="DisallowOverscroll" value="true"/>
<preference name="android-minSdkVersion" value="15" />
  <preference name="android-targetSdkVersion" value="26"/> //android 4.2
<preference name="android-build-tool" value="gradle" />
 
  <platform name="android">
   
  </platform>
  
  <access origin="*"/>
  <allow-intent href="http://*/*"/>
  <allow-intent href="https://*/*"/>
  <allow-intent href="tel:*"/>
  <allow-intent href="sms:*"/>
  <allow-intent href="mailto:*"/>
  <allow-intent href="geo:*"/>
  <platform name="android">
    <allow-intent href="market:*"/>
  </platform>
  <platform name="ios">
    <allow-intent href="itms:*"/>
    <allow-intent href="itms-apps:*"/>
  </platform>
</widget>
